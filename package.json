{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev:client": "vite dev --port 5000",
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "tsx script/build.ts",
    "start": "NODE_ENV=production node dist/index.cjs",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:push:sqlite": "drizzle-kit push --config=drizzle-sqlite.config.ts",
    "db:seed": "tsx server/seed.ts",
    "db:seed:demo": "tsx server/seed.ts",
    "db:clean": "tsx server/clean-demo-data.ts",
    "db:init:production": "tsx server/init-production-db.ts",
    "db:migrate": "tsx script/migrate.ts",
    "db:migrate:rollback": "tsx script/migrate.ts rollback",
    "db:migrate:indexes": "psql $DATABASE_URL -f migrations/add-performance-indexes.sql",
    "db:indexes:apply": "bash scripts/apply-indexes.sh",
    "db:indexes:test": "bash scripts/test-performance.sh",
    "db:analyze": "psql $DATABASE_URL -c 'ANALYZE;'",
    "test": "vitest run",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:changed": "vitest related --run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:smoke": "playwright test tests/e2e/smoke.spec.ts --reporter=list",
    "test:a11y": "playwright test tests/accessibility --reporter=html",
    "test:mobile": "playwright test tests/mobile --reporter=html",
    "test:responsive": "playwright test tests/responsive --reporter=html",
    "test:visual": "playwright test tests/visual --reporter=html",
    "test:performance": "playwright test tests/performance --reporter=html",
    "test:all": "npm run test && npm run test:e2e",
    "benchmark:security": "tsx server/performance/security-benchmarks.ts",
    "benchmark:all": "npm run benchmark:security",
    "analyze:a11y": "tsx scripts/analyze-a11y.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:a11y": "eslint client/src --ext .ts,.tsx --no-eslintrc --config eslint.config.js",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "npm run build && lhci autorun",
    "preview": "vite preview --port 4173",
    "deploy:staging": "bash scripts/deploy.sh staging",
    "deploy:production": "bash scripts/deploy.sh production",
    "rollback:staging": "bash scripts/rollback.sh staging",
    "rollback:production": "bash scripts/rollback.sh production",
    "docker:build": "docker build -t imobibase:latest .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "health:check": "curl -f http://localhost:5000/api/health || exit 1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:secret": "tsx scripts/generate-session-secret.ts",
    "rotate:secrets": "tsx scripts/rotate-secrets.ts",
    "validate:secrets": "tsx scripts/validate-secrets.ts",
    "prepare": "husky install"
  },
